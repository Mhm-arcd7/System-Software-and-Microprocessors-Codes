.model small
.data
n1 dw 1234h
n2 dw 4567h
.code
mov ax, @data
mov ds, ax
mov si,offset n1
mov ax,[si]
mov si,offset n2
mov bx,[si]
and ax,bx
mov cx, 04h
mov dx,0h
L1: mov bx,OAh

div bx
push dx
xor dx,dx
loop L1
mov cx, 04h
L2: pop dx
cmp dx,09h
jgt GT9
add dx,30h
jmp Display
Gt9: add dx, 37h
Display:mov ah, 02h
int 21h
loop L2
mov ah,4ch

int 21h
end
